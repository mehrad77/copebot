{
  "name": "copebot",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "setup": "npm install -g wrangler",
    "postsetup": "wrangler login",
    "predeploy": "webpack -c webpack.config.ts",
    "prestart": "webpack -c webpack.config.ts",
    "deploy": "wrangler deploy dist/worker.js",
    "start": "wrangler dev dist/worker.js --port 8443 --local-protocol https",
    "set-webhook": "ts-node set-webhook.ts"
  },
  "dependencies": {
    "@cfworker/web": "^1.14.0",
    "cfworker-middleware-telegraf": "^2.0.2",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250813.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "3.6.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
