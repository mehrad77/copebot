{
	"name": "copebot",
	"version": "0.0.0",
	"private": false,
	"type": "module",
  "scripts": {
    "setup": "npm install -g wrangler",
    "postsetup": "wrangler login",
    "predeploy": "webpack -c webpack.config.js",
    "prestart": "webpack -c webpack.config.js",
    "deploy": "wrangler deploy dist/worker.js",
    "start": "wrangler dev dist/worker.js --port 8443 --local-protocol https",
    "set-webhook": "node --loader ts-node/esm set-webhook.ts"
  },
  "dependencies": {
    "@cfworker/web": "^1.14.0",
    "cfworker-middleware-telegraf": "^2.0.2",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250813.0",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "3.6.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
